{
  "name": "gangs",
  "version": "1.2.1",
  "author": "Dalton Klein",
  "description": "Say goodbye to solo queuing, find competent team members to play with, make new friends",
  "main": "public/main.js",
  "window": {
    "frame": false,
    "toolbar": true
  },
  "private": true,
  "dependencies": {
    "@babel/core": "^7.13.0",
    "@electron/remote": "^2.0.9",
    "@reduxjs/toolkit": "^1.6.2",
    "@testing-library/jest-dom": "^5.15.0",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/react-tooltip": "^4.2.4",
    "electron-is": "^3.0.0",
    "electron-is-dev": "^2.0.0",
    "electron-log": "^4.4.8",
    "electron-updater": "^6.1.1",
    "gapi-script": "^1.2.0",
    "gsap": "^3.8.0",
    "moment": "^2.29.4",
    "node-sass": "^9.0.0",
    "primeicons": "^6.0.1",
    "primereact": "^9.6.0",
    "react": "^18.2.0",
    "react-beforeunload": "^2.5.3",
    "react-confetti": "^6.1.0",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "^18.2.0",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.10.0",
    "react-scripts": "^5.0.1",
    "react-select": "^5.5.0",
    "react-tooltip": "^5.16.1",
    "redux-observable": "^2.0.0",
    "redux-persist": "^6.0.0",
    "simple-peer": "^9.11.1",
    "socket.io-client": "^4.5.2",
    "styled-components": "^5.3.9",
    "typescript": "^4.9.5",
    "voice-activity-detection": "0.0.5",
    "web-vitals": "^1.1.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron:serve": "cross-env BROWSER=none electron .",
    "electron:build": "npm run build && electron-builder -c.extraMetadata.main=build/main.js",
    "electron:start": "electron ."
  },
  "homepage": "./",
  "repository": "https://github.com/Dalton-Klein/gangs-auto-updater",
  "publish": {
    "provider": "github",
    "releaseType": "release"
  },
  "build": {
    "extends": null,
    "appId": "gg.gangs",
    "files": [
      "dist/**/*",
      "build/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "directories": {
      "buildResources": "public",
      "output": "dist"
    },
    "win": {
      "icon": "build/assets/logo-v2-gangs.gg-transparent-white.png",
      "target": "nsis"
    },
    "publish": [
      {
        "provider": "github",
        "owner": "Dalton-Klein",
        "repo": "gangs-auto-updater"
      }
    ],
    "extraMetadata": {
      "main": "dist/main.js"
    },
    "buildVersion": "1"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/simple-peer": "^9.11.5",
    "@types/styled-components": "^5.1.26",
    "cross-env": "^7.0.3",
    "electron": "^25.2.0",
    "electron-builder": "^23.6.0",
    "remote-redux-devtools": "^0.5.16",
    "wait-on": "^7.0.1"
  },
  "proxy": "http://localhost:3010",
  "resolutions": {
    "styled-components": "^5"
  }
}
